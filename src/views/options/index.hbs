<style>
a {
  text-decoration: none;
  color: black;
}
</style>
<h2 class="mt-3 m-3" text><a href="/">Index</a></h1>
<div class="container">
    <div class="row">
        <table class="table table-hover">
  <tr class="trh">
		<th>ID</th>
		<th>Name</th>
		<th>Last Name</th>
		<th>Email</th>
		<th>Mobile</th>
		<th>Birth</th>
        <th colspan="2" class="text-center">Actions</th>
        <th></th>
	</tr>
    {{#each users}}
    <tr>
        <td>{{id}}</td>
        <td>{{nameu}}</td>
        <td>{{lnameu}}</td>
        <td>{{email}}</td>
        <td>{{mobile}}</td>
        <td>{{moment birth "YYYY-MM-DD"}}</td>
        {{!-- <td>{{moment format="DD/MM/YYYY"}}</td> --}}
        <td class="text-center"><a href="/edit/{{id}}" class="btn btn-info">Edit</a></td>
        <td class="text-center"><a href="/delete/{{id}}" class="btn btn-danger">Delete</a></td>
    </tr>
    {{/each}}
</table>
    </div>
</div>
